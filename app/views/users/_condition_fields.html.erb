<div class="field">
	
  <%= f.attribute_fields do |a| %>
    <%= a.attribute_select associations: [:foods] %>
  <% end %>
  <%= f.predicate_select compounds: false, only: [:cont, :not_cont, :eq, :not_eq] %>
  <%= f.value_fields do |v| %>
    <%= v.text_field :value %>


  <% end %>

  
  <%= link_to "remove", '#', class: "remove_fields" %>
</div>